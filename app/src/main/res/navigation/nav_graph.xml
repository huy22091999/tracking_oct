<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nav_allTrackingFragment">

    <fragment
        android:id="@+id/nav_allTrackingFragment"
        android:name="com.oceantech.tracking.ui.tracking.TrackingFragment"
        android:label="fragment_all_tracking"
        tools:layout="@layout/fragment_tracking" >
        <action
            android:id="@+id/action_nav_allTrackingFragment_to_nav_trackingFragment"
            app:destination="@id/nav_trackingFragment" />
        <action
            android:id="@+id/action_nav_allTrackingFragment_to_nav_timeSheetFragment"
            app:destination="@id/nav_timeSheetFragment" />
        <action
            android:id="@+id/action_nav_allTrackingFragment_to_userFragment"
            app:destination="@id/nav_userFragment" />
        <action
            android:id="@+id/action_nav_allTrackingFragment_to_detailUserFragment"
            app:destination="@id/nav_detailUserFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_trackingFragment"
        android:name="com.oceantech.tracking.ui.tracking.AddOrUpTrackFragment"
        android:label="fragment_tracking"
        tools:layout="@layout/fragment_add_or_up_tracking" >
        <argument
            android:name="id"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="content"
            app:argType="string"
            android:defaultValue='" "' />
        <action
            android:id="@+id/action_nav_trackingFragment_to_nav_allTrackingFragment"
            app:destination="@id/nav_allTrackingFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_timeSheetFragment"
        android:name="com.oceantech.tracking.ui.timesheet.TimeSheetFragment"
        android:label="fragment_time_sheet"
        tools:layout="@layout/fragment_time_sheet" >
    </fragment>
    <fragment
        android:id="@+id/nav_userFragment"
        android:name="com.oceantech.tracking.ui.user.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" >
        <action
            android:id="@+id/action_nav_userFragment_to_detailUserFragment"
            app:destination="@id/nav_detailUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_detailUserFragment"
        android:name="com.oceantech.tracking.ui.profile.DetailUserFragment"
        android:label="fragment_detail_user"
        tools:layout="@layout/fragment_detail_user" >
        <action
            android:id="@+id/action_detailUserFragment_to_nav_userFragment"
            app:destination="@id/nav_userFragment" />
        <argument
            android:name="info_user"
            app:argType="com.oceantech.tracking.data.model.User"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_nav_detailUserFragment_to_signinFragment2"
            app:destination="@id/signinFragment2" />
    </fragment>
    <fragment
        android:id="@+id/signinFragment2"
        android:name="com.oceantech.tracking.ui.security.SigninFragment"
        android:label="fragment_signin"
        tools:layout="@layout/fragment_signin" >
        <action
            android:id="@+id/action_signinFragment2_to_nextSigninFragment2"
            app:destination="@id/nextSigninFragment2" />
        <argument
            android:name="user"
            app:argType="com.oceantech.tracking.data.model.User"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/nextSigninFragment2"
        android:name="com.oceantech.tracking.ui.security.NextSigninFragment"
        android:label="fragment_next_signin"
        tools:layout="@layout/fragment_next_signin" >
        <action
            android:id="@+id/action_nextSigninFragment2_to_nav_detailUserFragment"
            app:destination="@id/nav_detailUserFragment" />
        <action
            android:id="@+id/action_nextSigninFragment2_to_signinFragment2"
            app:destination="@id/signinFragment2" />
        <argument
            android:name="user"
            app:argType="com.oceantech.tracking.data.model.User"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
</navigation>